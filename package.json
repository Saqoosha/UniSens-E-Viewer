{
  "name": "unisens-e-viewer",
  "description": "UniSens-E Data Viewer for DCL",
  "version": "0.2.0",
  "private": true,
  "main": "src/electron.js",
  "dependencies": {
    "apexcharts": "^3.8.6",
    "electron-window-state": "^5.0.3",
    "fsevents": "^1.2.9",
    "papaparse": "^5.1.0",
    "react": "^16.9.0",
    "react-apexcharts": "^1.3.3",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.9",
    "react-scripts": "3.1.2"
  },
  "homepage": "./",
  "author": {
    "name": "Saqoosha"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --mac --win"
  },
  "build": {
    "appId": "sh.saqoo.unisenseviewer",
    "productName": "UniSens-E Viewer",
    "files": [
      "./build/**/*",
      "./src/electron.js"
    ],
    "win": {
      "target": "zip"
    },
    "extends": null,
    "npmRebuild": false
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0"
  }
}
